{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [ true, "single" ],
        "no-console": false,
        "space-before-function-paren": true,
        "max-classes-per-file": false,
        "variable-name": false,
        "object-literal-sort-keys": false,
        "ban-types": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}
