{
  "name": "cocos-creator-3d",
  "version": "0.1.0",
  "description": "Cocos Creator 3D is a cross-platform 3D game engine written in Javascript, licensed under MIT.",
  "homepage": "http://www.cocos.com",
  "license": "MIT",
  "authors": [
    "AUTHORS.txt"
  ],
  "module": "index.js",
  "scripts": {
    "server": "http-server . -p 8002",
    "build": "npm run build:min && npm run build-declaration",
    "build:dev": "node ./rollup/out/build-engine-cli.js --sourcemap --platform=universal --physics=cannon --destination=./bin/cocos-3d.dev.js",
    "build:min": "node ./rollup/out/build-engine-cli.js --compress --sourcemap --platform=universal --physics=cannon --destination=./bin/cocos3d-js.min.js",
    "build-declaration": "node ./scripts/generate-declarations/generate-declarations-cli.js",
    "build-rollup": "tsc --build rollup/tsconfig.json",
    "postinstall": "npm run build-rollup",
    "build-api-json": "gulp build-api-json -i index.ts -o ./3d-api-docs/tempJson",
    "build-api": "npm run build-api-json && gulp build-3d-api -i index.ts -o ./3d-api-docs -j ./3d-api-docs/tempJson",
    "build-api-default": "gulp build-3d-api-default -i ./index.ts -o ./3d-api-docs/default"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cocos-creator/engine.git"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-transform-for-of": "^7.4.4",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/cannon": "github:cocos-creator/cannon.d.ts#cocos-master",
    "@types/fs-extra": "^5.0.4",
    "@types/uglify-js": "^3.0.4",
    "@types/yargs": "^12.0.5",
    "aliasify": "^2.1.0",
    "async": "1.5.2",
    "babel-preset-env": "^1.2.2",
    "babelify": "7.3.0",
    "browserify": "^16.2.3",
    "cannon": "github:cocos-creator/cannon.js#cocos-master",
    "chalk": "1.1.0",
    "commander": "^2.20.0",
    "dat.gui": "^0.7.5",
    "del": "1.2.0",
    "event-stream": "3.3.2",
    "fire-fs": "0.2.1",
    "fs-extra": "^7.0.1",
    "fs-jetpack": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-concat": "2.6.0",
    "gulp-fb": "0.5.1",
    "gulp-header": "1.2.2",
    "gulp-jshint": "1.11.2",
    "gulp-mirror": "0.4.0",
    "gulp-optimize-js": "^1.1.0",
    "gulp-plumber": "0.6.6",
    "gulp-rename": "1.2.2",
    "gulp-shell": "^0.6.5",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "3.0.6",
    "http-server": "^0.11.1",
    "jshint-stylish": "2.1.0",
    "multipipe": "0.3.0",
    "persistify": "^1.1.1",
    "require-dir": "0.3.0",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.1.0-1",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-watch": "^4.3.1",
    "tfig": "^1.0.20",
    "tslint": "^5.12.0",
    "tween.js": "github:cocos-creator/tween.js#cocos-master",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typedoc-plugin-localization": "github:cocos-creator/typedoc-plugin-localization#master",
    "typescript": "^3.4.4",
    "vconsole": "^3.3.0",
    "vinyl": "1.1.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.0.0",
    "watchify": "^3.7.0",
    "yargs": "^12.0.5"
  }
}
